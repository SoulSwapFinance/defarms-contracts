    "deploy:mock": "forge script script/MockToken.s.sol:TokenScript --rpc-url https://rpc.ankr.com/avalanche --broadcast --verify",
    "test": "forge test -v",
    "test:more": "forge test -vv",
    "test:most": "forge test -vvvvv",
    "flatten": "forge flatten"
