  "author": "Buns <bunsthedev@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "forge build",
    "deploy": "forge script script/Manifester.s.sol:ManifesterScript --rpc-url https://rpc.ankr.com/avalanche --broadcast --verify",
